<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Toolbar">
        <Properties>
          <Property
            Name="MarginBottom">0px</Property>
          <Property
            Name="MarginRight">0px</Property>
        </Properties>
      </Style>
      <Medium
        Name="Mobile">
        <Style
          Name="Toolbar">
          <Properties>
            <Property
              Name="Display">none</Property>
          </Properties>
        </Style>
      </Medium>
      <Style
        Name="ToolbarItem">
        <Classes>
          <StyleClass
            Name="NextTopic">
            <Properties>
              <Property
                Name="BackgroundImage">url('icon-arrow-purple.png')</Property>
              <Property
                Name="Width">52px</Property>
              <Property
                Name="Height">48px</Property>
              <Property
                Name="BackgroundColor">transparent</Property>
              <Property
                Name="BackgroundGradient">vertical transparent transparent</Property>
              <Property
                Name="BoxShadowColor">#ffffff</Property>
              <Property
                Name="BorderTop">none 0px #e1e1e1</Property>
              <Property
                Name="BorderRight">none 0px #e1e1e1</Property>
              <Property
                Name="BorderBottom">none 0px #e1e1e1</Property>
              <Property
                Name="BorderLeft">none 0px #e1e1e1</Property>
              <Property
                Name="BoxShadowVShadow">0px</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="icon-arrow-purple.png"></Resource>
    </Resources>
  </Stylesheet>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="NextTopic" />
  <Strings />
</CatapultSkin>